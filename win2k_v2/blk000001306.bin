<script language="javascript">

// Misc. variables
var gszLastBtn;
var giTotalButtons;

// Taskpad static text
var gszTaskpadTitle;
var gszTaskpadDescription;

// Watermark
var gszTaskpadWatermark;
var giWatermarkDisplayObjectType;

// Tooltips
var gaszBtnTooltips = new Array();

// Button object
var gaiBtnObjectType = new Array();
//	values are:
//		0 = undefined
//		1 = Symbol
//		2 = GIF - Index 0 transparent
//		3 = GIF - Index 1 transparent
//		4 = Non-transparent raster image

// Button elements
var gaszSymbolString = new Array();
var gaszFontFamilyName = new Array();
var gaszURLtoEOT = new Array();
var gaszBtnCaptions = new Array();
var gaszBtnOffBitmap = new Array();
var gaszBtnOverBitmap = new Array();
var gaszBtnSymbols = new Array();

// Button actions
var gaiBtnActionType = new Array();
var gaiBtnActionID = new Array();
var gaszBtnActionClsid = new Array();
var gaszBtnActionURL = new Array();
var gaszBtnActionScript = new Array();
var gaszBtnActionScriptLanguage = new Array();

// Button utility variables
var gaszFontFamilyNameUnique = new Array();
var gaszURLtoEOTUnique = new Array();

// Global listview caption and button object variables
var gbShowLVTitle = false;
var gszLVTitle;
var gbHasLVButton = false; // Show the ListView Button?
var gszLVBtnCaption;
var gszLVBtnClsid;
var giLVBtnNotifyID;

// Enumerate the CIC control
var objTaskArray = new Array();
var szHash = location.hash;

if (szHash != "") {
	szHash = szHash.substr(1);
}

// Get the taskpad title
gszTaskpadTitle  = MMCCtrl.GetTitle(szHash);

// Get taskpad description;
// accepts text or properly formatted HTML, e.g. <br> or <b></b>
gszTaskpadDescription = MMCCtrl.GetDescriptiveText(szHash);

// NOTE: Branding is *not* used on this page.
// var branding   = MMCCtrl.GetBranding(szHash);

var objDisplay;
var objTask = MMCCtrl.GetFirstTask(szHash);
var iNumTasks = -1;

while (objTask != null) {
	iNumTasks++;
	objTaskArray[iNumTasks] = objTask;
	objTask = MMCCtrl.GetNextTask();
}

// Keep track of the total number of buttons
giTotalButtons = iNumTasks;

//*************
// TASK BUTTONS
//*************

for (i=0; i <= giTotalButtons; i++) {		
	objTask = objTaskArray[i];
	gaszBtnCaptions[i] = objTask.Text;
	gaszBtnTooltips[i] = objTask.Help;
	gaiBtnActionType[i] = objTask.ActionType;
	gaszBtnActionClsid[i] = objTask.Clsid;

	switch (objTask.ActionType)
	// 3 possibilities for action:  "ID:", "LINK:" and "SCRIPT:"
	{
		case 0:
			// MMC_TASK_ACTION_ID
			gaiBtnActionID[i] = objTask.CommandID;
			break;

		case 1:
			// MMC_TASK_ACTION_LINK
			gaszBtnActionURL[i] = objTask.ActionURL;
			break;

		case 2:
			// MMC_TASK_ACTION_SCRIPT
			gaszBtnActionScript[i] = objTask.Script;
			gaszBtnActionScriptLanguage[i] = objTask.ScriptLanguage;
			break;
	}

	objDisplay = objTask.DisplayObject;
	gaiBtnObjectType[i] = objDisplay.DisplayObjectType;
	
	switch (objDisplay.DisplayObjectType)
	{
		case 1:  // MMC_TASK_DISPLAY_TYPE_SYMBOL
			gszTaskpadWatermark = objDisplay.SymbolString
			// Note: objDisplay.FontFamilyName below is actually returning the
			// plain URLtoEOT filename (e.g. GLYPH100.EOT)
			gaszFontFamilyName[i] = objDisplay.FontFamilyName;
			gaszSymbolString[i] = objDisplay.SymbolString;
			gaszURLtoEOT[i] = objDisplay.URLtoEOT;
			break;

		case 2:  // MMC_TASK_DISPLAY_TYPE_VANILLA_GIF, index 0 is transparent
		case 3:  // MMC_TASK_DISPLAY_TYPE_CHOCOLATE_GIF, index 1 is transparent
		case 4:  // MMC_TASK_DISPLAY_TYPE_BITMAP, non-transparent raster
			gaszBtnOffBitmap[i] = objDisplay.MouseOffBitmap;
			// Don't assign MouseOverBitmap unless it is different from MouseOffBitmap
			if (objDisplay.MouseOffBitmap.toUpperCase() != objDisplay.MouseOverBitmap.toUpperCase()) {
				gaszBtnOverBitmap[i] = objDisplay.MouseOverBitmap;
			}
			break;
	}
}

//******************
// LISTVIEW ELEMENTS
//******************

var objListview = MMCCtrl.GetListPadInfo(szHash);

if (objListview) {
	gbShowLVTitle = true;
	gszLVTitle = objListview.Title;
	if (objListview.HasButton) {
		gbHasLVButton = true;
		gszLVBtnCaption = objListview.Text;
		giLVBtnNotifyID = objListview.NotifyID;
		gszLVBtnClsid = objListview.Clsid;
	}
}

//**********
// WATERMARK
//**********

var objWatermark = MMCCtrl.GetBackground( szHash );

if (objWatermark != null) {
	// Keep track of the watermark display object type
	giWatermarkObjectType = objWatermark.DisplayObjectType;
	
	switch (objWatermark.DisplayObjectType)
	{
		case 1: // MMC_TASK_DISPLAY_TYPE_SYMBOL
			var gszWatermarkFontFamily = objWatermark.FontFamilyName;
			var gszWatermarkSymbolString = objWatermark.SymbolString;
      break;
		case 2: // MMC_TASK_DISPLAY_TYPE_VANILLA_GIF,      // (GIF) index 0 is transparent
		case 3: // MMC_TASK_DISPLAY_TYPE_CHOCOLATE_GIF,    // (GIF) index 1 is transparent
		case 4: // MMC_TASK_DISPLAY_TYPE_BITMAP            // non-transparent raster
			// Note: no distinction made between GIF & Raster for the watermark
			var gszWatermarkImage = objWatermark.MouseOverBitmap;
	}
}

//***********************************************
// ADD @FONT-FACE DEFINITION TO DYMANIC STYLE TAG
//***********************************************

// Determine which EOTs are unique, and build up new arrays
// with 1 entry for each unique EOT and FontFamilyName

for (var i = 0; i <= giTotalButtons; i++) {
	if (typeof(gaszURLtoEOT[i]) == "string") {
		// If the EOT is unique, populate the unique EOT and FontFamilyName arrays
		if (IsUniqueEOT(gaszURLtoEOT[i]) == true) {
			// Add unique EOT & Font Family Names to their respective arrays
			AddUniqueEOT (gaszURLtoEOT[i], gaszFontFamilyName[i]);
		}		
	}
}

// Add @font-face definitions to a new STYLE tag (if necessary)
if (gaszURLtoEOTUnique.length > 0) {
	var szFontFaceDef = "<style>\n";

	// Loop through each unique EOT & FontFamilyName
	for (var i = 0; i < gaszURLtoEOTUnique.length; i++) {
		szFontFaceDef += "@font-face\n";
		szFontFaceDef += "{\n";
		szFontFaceDef += "font-family: " + gaszFontFamilyNameUnique[i] + ";\n";
		szFontFaceDef += "src: url(" + gaszURLtoEOTUnique[i] + ");\n";
		szFontFaceDef += "}\n";
	}
	
	szFontFaceDef += "</style>";

	// Use document.write() to add the STYLE tag
	// since there is no object model for manipulating
	// EOT style sheet entries.
	document.write (szFontFaceDef);
}

//***********************************
// EOT & FONT-FAMILY HELPER FUNCTIONS
//***********************************

function IsUniqueEOT(szURLtoEOT)
{
	// Get the length of the test array
	var iLength = gaszURLtoEOTUnique.length;

	// If the length is empty, return true
	// since the EOT *must* be unique
	if (iLength == 0) {
		return true;
	}
	
	// Compare with each existing entry in the array
	for (var i = 0; i < iLength; i++) {
		if (gaszURLtoEOTUnique[i] == szURLtoEOT) {
			// Found a duplicate
			return false;
		}
	}
	
	// If we made it this far, the EOT is unique
	return true;
}

function AddUniqueEOT(szEOT, szFontFamilyName)
{
	// Use the length of the EOT array to get the
	// index for the next element to be added
	var iNextIndex = gaszURLtoEOTUnique.length;
	gaszURLtoEOTUnique[iNextIndex] = szEOT;
	gaszFontFamilyNameUnique[iNextIndex] = szFontFamilyName;
}

</script>

</head>

