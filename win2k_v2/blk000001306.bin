<script language="jscript">

// Store document margins in a global var
with (document.body)
{
	var giMargins = parseInt(topMargin) + parseInt(bottomMargin);
}


function GetElementIndex(ElementID)
{
	// Purpose: Given an Element ID formatted as follows:
	//         "divCaption_12"
	// returns the numeric (integer) portion after the underscore character;
	// returns -1 if delimiter not found.
	
	var iDelimitLoc = ElementID.indexOf ("_");

	if (iDelimitLoc == -1) {
		// Return -1 if delimiter not found (which shouldn't happen)
		return iDelimitLoc;
	}
	else {
		var iTheIndex = ElementID.substring (iDelimitLoc + 1, ElementID.length);
		return parseInt(iTheIndex);
	}
}


function IsStillOverButton()
{
	//  Purpose: Determines if a mouseover or mouseout event
	//  was fired over the same button (indicating that the pointer
	//  is still over the button and that highlighting/unhighlighting
	//  should be ignored).
	// 
	//  Returns true if and only if:
	//    * both fromElement and toElement are not null;
	//    * both classNames match;
	//    * both element IDs match.
	
	var fromX = window.event.fromElement;
	var toX = window.event.toElement;

	// Trap case where mouse pointer appeared over a button out of nowhere
	// (e.g. as a result of switching focus from another app).
	if ((fromX != null) && (toX != null)) {
		// return true if moving within elements of a single button
		if (fromX.className == toX.className) {
			if (GetElementIndex(fromX.id) == GetElementIndex(toX.id)) {
				return true;
			}
		}
	}
	return false;
}

//****************************************
// BUTTON HIGHLIGHT/UNHIGHLIGHT FUNCTIONS
//****************************************

function HighlightButton(szBtnIndex)
{
	document.all("spanSymbol_" + szBtnIndex).style.color = "blue";
	with (document.all("anchorCaption_" + szBtnIndex).style)
	{
		color = "blue";
		textDecoration = "underline";
	}
}

function UnhighlightButton()
{
	if (gszLastBtn >= 0) {
		document.all("spanSymbol_" + gszLastBtn).style.color = "windowtext";
		with (document.all("anchorCaption_" + gszLastBtn).style)
		{
			color = "windowtext";
			textDecoration = "none";
		}
	}
}

//**********************************
// RESIZE / REPOSITION FUNCTIONS
//**********************************

function ResizeListview()
{
	// NOTE: Only need to resize listview vertically
	
	var ch = document.body.clientHeight;

	// Bail out if at or below minimum size
	if (ch <= 450) {
		if (lv.offsetHeight < 338) {
			lv.style.height = 338;
		}
		return;
	}

	// Size the listview
	lv.style.height = ch - tdDescriptiveText.offsetHeight - tdLVTitleContainer.offsetHeight - tdTitle.offsetHeight - giMargins - 10;

}

function SetCaptionPanelText()
{
	// Purpose: Adds an elipse (e.g. "...") to the end of the listview title caption
	// if estimated width exceeds available space.
	
	// Calculate display width in listview title:
	// (listview width - button width - padding) / L_iPixelsPerCharAt8pt_Number
	// English = 6 (e.g. assum 6 pixels per char @ 8pt)	
	
	// Localization note: specify the average # of pixels per character at a font size of 8pt
	var L_iPixelsPerCharAt8pt_Number = 6;
	var iDisplayWidth = (lv.offsetWidth - btnLV.offsetWidth - 20) / L_iPixelsPerCharAt8pt_Number;	
	
	if (gszLVTitle.length > iDisplayWidth) {
		// Add elipse to title
		var szElipseTitle = gszLVTitle.substring(0, iDisplayWidth) + "...";
		tdLVTitle.innerText = szElipseTitle;
		// Show complete title in tooltip
		tdLVTitle.title = gszLVTitle;
	}
	else {
		// No need to show elipse
		tdLVTitle.innerText = gszLVTitle;
		// Tooltip is empty if elipse isn't shown
		tdLVTitle.title = "";
	}
}


function insertTaskSymbols()
{
	// Build up the HTML
	var strTable = '';
	
	strTable += '<table width="250" cellspacing="2" cellpadding="0" border="0" frame="box" rules="all">\n';
	
	if (giTotalButtons == -1) {
		strTable += ' <tr><td colspan="2"></td></tr>\n';
	}
	else {
		for (var i = 0; i <= giTotalButtons; i++) {
			if ((i % 2) == 0) {
				// start TR
				strTable += ' <tr>\n';
			}
		
			strTable += '  <td width="50%" align="center" valign="top" class="tdSymbolContainer">\n';
			strTable += '   <table id="tblSymbol_' + i + '" width="100%" cellspacing="0" cellpadding="0" border="0" frame="none" rules="none" title="' + gaszBtnTooltips[i] + '">\n';
			strTable += '    <tr>\n';
			strTable += '     <td align="center" valign="top" nowrap class="tdSymbol">\n';
			strTable += '      <span id="spanSymbol_' + i + '" class="clsSymbolBtnDisabled" style="font-family:' + gaszFontFamilyName[i] + ';font-weight:normal;font-size:36pt;">' + gaszSymbolString[i] + '</span>\n';
			strTable += '     </td>\n';
			strTable += '    </tr>\n';
			strTable += '    <tr>\n';
			strTable += '     <td width="100%" align="center" valign="top" class="tdSymbol">\n';
			strTable += '      <a href="" id="anchorCaption_' + i + '" class="clsSymbolBtnDisabled">' + gaszBtnCaptions[i] + '</a>\n';
			strTable += '     </td>\n';
			strTable += '    </tr>\n';
			strTable += '   </table>\n';
			strTable += '  </td>\n';
		
			// End TR
			if ((i % 2) == 1) {
				strTable += ' </tr>\n';
			}	
		}
	}

	// Add a blank cell if total # of cells is odd
	if ((giTotalButtons % 2) == 0) {
		strTable += '  <td>&nbsp;</td>\n';
		strTable += ' </tr>\n';
	}

	strTable += '</table>\n';
	
	// Insert HTML
	tdSymbolContainer.innerHTML = strTable;
}


//****************************		
// CLICK EVENT HELPER FUNCTION
//****************************

function SymbolClickHandler(iTheIndex)
{

	switch (gaiBtnActionType[iTheIndex])
	{
		case 0:
			// MMC_TASK_ACTION_ID:
			ExecuteCommandID (gaszBtnActionClsid[iTheIndex], gaiBtnActionID[iTheIndex], 0);
			break;

		case 1:
			// MMC_TASK_ACTION_LINK:
			// document.location(gaszBtnActionURL[iTheIndex]);
			window.navigate (gaszBtnActionURL[iTheIndex]);
			break;

		case 2:
			// MMC_TASK_ACTION_SCRIPT:
			// Convert script language string to upper case
			var szLanguage = gaszBtnActionScriptLanguage[iTheIndex].toUpperCase();
						
			switch (szLanguage)
			{
				case "JSCRIPT":
				case "JAVASCRIPT":
					// Pass a script string to the JSObject to be evaluated and executed
					// through the eval method (this can be a semi-colon delimited complex expression)
					eval (gaszBtnActionScript[iTheIndex]);
					break;

				case "VBSCRIPT":
				case "VBS":
					// Use the window.execScript method to execute a simple or complex VBScript expression
					window.execScript (gaszBtnActionScript[iTheIndex], szLanguage);
					break;

				default:
					var L_UnknownScriptingLanguage_ErrorMessage = "Unrecognized scripting language.";
					alert (L_UnknownScriptingLanguage_ErrorMessage);
					break;
			}
			break;

		default:
			var L_UnknownTask_ErrorMessage = "Unrecognized task.";
			alert (L_UnknownTask_ErrorMessage);
			break;
	}
}

//***************************************************
// CIC TASK NOTIFY HELPER FUNCTION (ExecuteCommandID)
//***************************************************

function ExecuteCommandID(szClsid, arg, param)
{
   MMCCtrl.TaskNotify (szClsid, arg, param);
}


//****************
// EVENT HANDLERS
//****************

function handleMouseOver()
{
	var el = window.event.srcElement;
	
	switch (el.className)
	{
		case "clsSymbolBtn":
			window.status = "";
			// Bail out if we're still over the same button
			if (IsStillOverButton() == true) {
				window.event.cancelBubble = true;
			}
			else {
				UnhighlightButton();
				szBtnIndex = GetElementIndex(el.id);
				HighlightButton(szBtnIndex);
				// Keep track of highlighted button index;
				gszLastBtn = szBtnIndex;
			}
			break;
	}
}

function handleMouseOut()
{
	var el = window.event.srcElement;
	
	if (el.className == "clsSymbolBtn")
	{
		// Bail out if we're still over the same button
		if (IsStillOverButton() == true) {
			window.event.cancelBubble = true;
		}
		else {
			// We're really leaving the button
			UnhighlightButton();
		}
	}
}

function handleClick()
{
	var el = window.event.srcElement;
	
	switch (el.className)
	{
		case "clsSymbolBtn":
			// Cancel the default navigation action
			window.event.returnValue = false;
			// Call helper function to process the click
			var iTheIndex = GetElementIndex(el.id);
			SymbolClickHandler(iTheIndex);
			break;

		case "btnLV":
			// Cancel the default navigation action
			window.event.returnValue = false;
			// Call the TaskNotify helper function
			ExecuteCommandID(gszLVBtnClsid, giLVBtnNotifyID, 0);
			break;

		case "clsSymbolBtnDisabled":
			// Cancel the default navigation action
			window.event.returnValue = false;
			break;			

		default:
			// Stub
			break;
	}
}


function handleBlur()
{
	UnhighlightButton();
}


function handleTaskEnable(iTaskIndex, bEnable)
{
	if (bEnable) {
		// ENABLE THE TASK BUTTON
		with (document.all("spanSymbol_" + iTaskIndex))
		{
            className = "clsSymbolBtn";
			style.color = "windowtext";
			style.cursor = "hand";
		}
		with (document.all("anchorCaption_" + iTaskIndex))
		{
            className = "clsSymbolBtn";
			style.color = "windowtext";
			style.cursor = "hand";
			// Add the anchor to the tab order
			tabIndex = 0;
		}
	}
	
	else {
		// DISABLE THE TASK BUTTON
		with (document.all("spanSymbol_" + iTaskIndex))
		{
            className = "clsSymbolBtnDisabled";
			style.color = "inactiveborder";
			style.cursor = "default";
		}
		with (document.all("anchorCaption_" + iTaskIndex))
		{
            className = "clsSymbolBtnDisabled";
			style.color = "inactiveborder";
			style.cursor = "default";
			// Remove anchor from tab order
			tabIndex = -1;
		}
	}
}


function handleResize()
{
	ResizeListview();
	SetCaptionPanelText();
}

//**************************
// PENULTIMATE LOAD HANDLER
//**************************

function handleLoad()
{
	insertTaskSymbols();
	handleResize();
}

//*************************
// EVENT FUNCTION POINTERS
//*************************

document.onmouseover = handleMouseOver;
document.onmouseout = handleMouseOut;
document.onclick = handleClick;
window.onresize = handleResize;
window.onblur = handleBlur;
window.onload = handleLoad;

</script>

<script language="JScript" for="MMCCtrl" event="TaskEnable(bEnable, iTaskIndex)">
	// Fires when the enabled state for an individual button needs to be toggled
	handleTaskEnable(iTaskIndex, bEnable)
</script>

</body>

