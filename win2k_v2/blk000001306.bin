<div id="divMaster" class="divMaster"></div>


<script language="jscript">

//*************************************
// GLOBALS
//*************************************

var giLinkCount;
var gszFramesetSize = "150,*";

// Flag for determining where navigation was fired from
// (either scope pane or taskpad)
var gbScopePaneNavigate = true;

function GetLinkCount()
{
	// Get the link count of the sibling NavList for this page
	giLinkCount = oLinks.Count;
	
	// Bail out if there's not at least 1 link
	if (giLinkCount <= 0) {
		// Inform the user
		var L_InvalidOLinksCount_ErrorMessage = "Invalid oLinks.Count property: ";
		alert (L_InvalidOLinksCount_ErrorMessage);
		return false;
	}
	switch (giLinkCount) {
	
		case 1:
			// There's only 1 link, so show the taskpad page in full (orphan) mode
			with (window.top.frames("Frame_NavbarSibling").document)
			{
				// Disable frame resizing
				all.tags("FRAME").item(0).noResize = true;
				// Collapse the sibling nav list
				body.cols = "0,*";
			}
			break;
						
		default:
			// There are 2 or more links, so show the sibling NavList			
			with (window.top.frames("Frame_NavbarSibling").document)
			{
				// Enable frame resizing
				all.tags("FRAME").item(0).noResize = false;
				// Set frame width to saved size
				body.cols = gszFramesetSize;
			}
			break;
	}
	
	return true;								
}


function BuildVerticalTable()
{
	// Start by clearing out divMaster
	divMaster.innerHTML = "";

	// Build up the new HTML to insert into divMaster
	var szTable = '<table width="100%" cellspacing="0" cellpadding="5" border="0" frame="box" rules="all">\n';

	for (var i = 0; i < giLinkCount; i++) {
		szTable += '<tr>\n';
		szTable += '		<td id="tdPointer_' + i +'" class="tdPointer" width="5">4</td>\n';
		szTable += '    <td LinkItem id="td_' + i + '" class="UnSelected" title="">\n';
		szTable += '    ' + oLinks.Item(i + 1).Caption +'\n';
		szTable += '    </td>\n';
		szTable += '</tr>\n';
	}
	szTable += '</table>\n';
	
	divMaster.insertAdjacentHTML ("AfterBegin", szTable);	
}


function GetSelectedItemIndex()
{
	return oLinks.SelectedItem.Index;
}


function SetSelection()
{
	// Brute-force resetting of all items to default state
	for (var i = 0; i < giLinkCount; i++) {
		document.all("td_" + i).className = "UnSelected";
		document.all("tdPointer_" + i).style.visibility = "hidden";
	}

	var iSelectedItemIndex = GetSelectedItemIndex();

	// Show the pointing arrow
	document.all("tdPointer_" + (iSelectedItemIndex - 1)).style.visibility = "visible";

	with (document.all("td_" + (iSelectedItemIndex - 1)))
	{
		// Select the selected item
		className = "Selected";
		// Scroll the selected item into view		
		scrollIntoView(true);
		// if click was not fired from scope pane
		if (gbScopePaneNavigate == false) {
			focus();
			gbScopePaneNavigate = true;
		}
	}	
}


function DoNavigate()
{
	self.parent.frames("Frame_Taskpad").frames("Taskpad").location.href = oLinks.SelectedItem.TargetURL;
}


//************************************
// UTILITY FUNCTIONS
//************************************

function GetElementIndex(ElementID)
{
	// Purpose: Given an Element ID formatted as follows:
	//         "divCaption_12"
	// returns the numeric (integer) portion after the underscore character;
	// returns -1 if delimiter not found.

	var iDelimitLoc = ElementID.indexOf("_");
	
	if (iDelimitLoc == -1) {
		// Return -1 if delimiter not found (which shouldn't happen)
		return iDelimitLoc;
	}
	else {
		var theIndex = ElementID.substring(iDelimitLoc + 1, ElementID.length);

		// Convert to a number and return
		return parseInt(theIndex);
	}
}


//************************************
// EVENT HANDLERS
//************************************

function handleClick()
{
	var el = window.event.srcElement;

	switch (el.tagName) {
		case "TD":
			// Bail out if user clicked the currently selected cell
			if (el.className == "Selected") {
				return;
			}

			// Get the index of the clicked item
			var iTheIndex = GetElementIndex(el.id);

			//**************************************************
			// Change the selected property for the clicked item
			//**************************************************
			
			// Set boolean flag to indicate click was not fired from scope pane
			gbScopePaneNavigate = false;

			// Changing this property implicitly notifies the parent control
			// that it needs send an update notification to MMC
			oLinks.Item(iTheIndex + 1).Selected = true;
			break;
			
		default:	
			//Stub;
			break;
	}
}


function handleMouseOver()
{
	var el = window.event.srcElement;

	switch (el.tagName) {
		case "TD":
			// Bail out if we're over the pointer symbol
			if (el.getAttribute("LinkItem") == null) {
				return;
			}
			
			// Show TooltipText
			var iTheIndex = GetElementIndex(el.id);
			el.setAttribute("title", oLinks.Item(iTheIndex + 1).TooltipText);

			// Set CSS className
			if (el.className == "Selected") {
				return;
			}
			el.className = "Hover";
			break;
	}
}


function handleMouseOut()
{
	var el = window.event.srcElement;

	switch (el.tagName) {
		case "TD":
			// Bail out if we're over the pointer symbol
			if (el.getAttribute("LinkItem") == null) {
				return;
			}

			// Set CSS className
			if (el.className == "Selected") {
				return;
			}
			
			el.className = "UnSelected";
			break;
	}
}


function handleLoad()
{
	// Set the parentage of the NavCtrl
	// 0 = sibling navlist; 1 = parent navlist
	oLinks.Parentage = 0;
}

//************************************
// EVENT FUNCTION POINTERS
//************************************

document.onclick = handleClick;
document.onmouseover = handleMouseOver;
document.onmouseout = handleMouseOut;
window.onload = handleLoad;

</script>

<!--CUSTOM EVENT HANDLERS-->

<script language="JScript" for="oLinks" event="Navigate(iTheIndex)">
	// Fires when a new sibling node has been selected in MMC
	SetSelection();
	DoNavigate();
</script>

<script language="JScript" for="oLinks" event="ListChange()">
	// Fires when a new parent node has been selected in MMC and
	// the entire sibling nav list needs to be re-enumerated
	var bRetVal = GetLinkCount();
		
	if (bRetVal) {
		BuildVerticalTable();
		SetSelection();
	}	
</script>

</body>

