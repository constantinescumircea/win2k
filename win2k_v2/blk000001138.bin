[Version]
Signature   = "$Windows NT$"
Class       = Net
ClassGUID   = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider    = %Msft%
LayoutFile  = layout.inf
DriverVer=10/19/1999,2.30.0.0

[Manufacturer]
%Intel% = Intel

[ControlFlags]
ExcludeFromSelect = PCI\VEN_8086&DEV_5201&SUBSYS_00018086&REV_01, \
                    PCI\VEN_8086&DEV_5201&SUBSYS_00018086&REV_03, \
                    PCI\VEN_8086&DEV_5201
                    
[Intel]
; DisplayName                                    Section         DeviceID
; -----------                                    -------         --------
%E100s.DeviceDesc% = E100SERVER.ndi, PCI\VEN_8086&DEV_5201&SUBSYS_00018086&REV_01
%E100s.DeviceDesc% = E100SERVER.ndi, PCI\VEN_8086&DEV_5201&SUBSYS_00018086&REV_03
%E100s.DeviceDesc% = E100SERVER.ndi, PCI\VEN_8086&DEV_5201
 
;-------------------------------------------------------------------------------
; Intel(R) PRO/100 Intelligent Server Adapter
;
[E100Server.ndi]
Characteristics = 0x84 ; NCF_HAS_UI | NCF_PHYSICAL
BusType         = 5    ; PCI
AddReg          = E100Server.reg
CopyFiles       = E100Server.CopyFiles

[E100Server.ndi.Services]
AddService      = E100SERVER, 2, E100Server.Service, common.EventLog

;-----------------------------------------------------------------------------
; Intel(R) PRO/100 Intelligent Server Adapter Parameters
;
[E100Server.reg]
HKR, Ndi\Interfaces,                   UpperRange, 0, "ndis5"
HKR, Ndi\Interfaces,                   LowerRange, 0, "ethernet"
HKR, Ndi,                              Service,    0, "E100SERVER"

; Speed
HKR, Ndi\Params\Speed,                 ParamDesc,  0, %Speed%
HKR, Ndi\Params\Speed,                 Default,    0, "0"
HKR, Ndi\Params\Speed,                 Type,       0, "enum"
HKR, Ndi\Params\Speed\Enum,            "0",        0, %AutoDetect%
HKR, Ndi\Params\Speed\Enum,            "10",       0, %10Mb%
HKR, Ndi\Params\Speed\Enum,            "100",      0, %100Mb%

; ForceDpx
HKR, Ndi\Params\ForceDpx,              ParamDesc,  0, %ForceDpx%
HKR, Ndi\Params\ForceDpx,              Default,    0, "0"
HKR, Ndi\Params\ForceDpx,              Type,       0, "enum"
HKR, Ndi\Params\ForceDpx\Enum,         "0",        0, %AutoDetect%
HKR, Ndi\Params\ForceDpx\Enum,         "1",        0, %Half-Duplex%
HKR, Ndi\Params\ForceDpx\Enum,         "2",        0, %Full-Duplex%

; Rx Frame Descriptors
HKR, Ndi\Params\NumRfd,                ParamDesc,  0, %NumRfd% 
HKR, Ndi\Params\NumRfd,                Default,    0, "32"
HKR, Ndi\Params\NumRfd,                Min,        0, "8"
HKR, Ndi\Params\NumRfd,                Max,        0, "128"
HKR, Ndi\Params\NumRfd,                Step,       0, "1"
HKR, Ndi\Params\NumRfd,                Base,       0, "10"
HKR, Ndi\Params\NumRfd,                Type,       0, "int"

; Tx Control Blocks
HKR, Ndi\Params\NumTcb,                ParamDesc,  0, %NumTcb% 
HKR, Ndi\Params\NumTcb,                Default,    0, "16"
HKR, Ndi\Params\NumTcb,                Min,        0, "8"
HKR, Ndi\Params\NumTcb,                Max,        0, "80"
HKR, Ndi\Params\NumTcb,                Step,       0, "1"
HKR, Ndi\Params\NumTcb,                Base,       0, "10"
HKR, Ndi\Params\NumTcb,                Type,       0, "int"

; Coalesce Buffers
HKR, Ndi\Params\NumCoalesce,           ParamDesc,  0, %NumCoalesce% 
HKR, Ndi\Params\NumCoalesce,           Default,    0, "8"
HKR, Ndi\Params\NumCoalesce,           Min,        0, "4"
HKR, Ndi\Params\NumCoalesce,           Max,        0, "32"
HKR, Ndi\Params\NumCoalesce,           Step,       0, "1"
HKR, Ndi\Params\NumCoalesce,           Base,       0, "10"
HKR, Ndi\Params\NumCoalesce,           Type,       0, "int"

; Connector
HKR, Ndi\Params\Connector,             ParamDesc,  0, %Connector% 
HKR, Ndi\Params\Connector,             Default,    0, "0"
HKR, Ndi\Params\Connector,             Type,       0, "enum"
HKR, Ndi\Params\Connector\Enum,        "0",        0, %AutoDetect%
HKR, Ndi\Params\Connector\Enum,        "1",        0, %TPE%
HKR, Ndi\Params\Connector\Enum,        "2",        0, %MII%

; Networkaddress
HKR, NDI\params\NetworkAddress,        ParamDesc,      0, %NetworkAddress%
HKR, NDI\params\NetworkAddress,        type,           0, "edit"
HKR, NDI\params\NetworkAddress,        LimitText,      0, "12"
HKR, NDI\params\NetworkAddress,        UpperCase,      0, "1"
HKR, NDI\params\NetworkAddress,        default,        0, " "
HKR, NDI\params\NetworkAddress,        optional,       0, "1"

; Hardwired defaults
HKR, ,                          Adaptive_IFS,      0, "1"
HKR, ,                          Congest,           0, "0"
HKR,,                           CPUSaver,          0, "1"
HKR, ,                          Dynamic_Threshold, 0, "1"
HKR, ,                          IFSValue,          0, "0"
HKR, ,                          MapRegisters,      0, "64"
HKR, ,                          MsPciScan,         0, "2"
HKR, ,                          NumTbdPerTcb,      0, "8"
HKR, ,                          PerfOptims,        0, "14"
HKR, ,                          RxDmaCount,        0, "0"
HKR, ,                          RxFifo,            0, "8"
HKR,,                           RxPacketCount,     0, "5"
HKR,,                           RxTimerInterval,   0, "500"
HKR, ,                          Threshold,         0, "16"
HKR, ,                          TxDmaCount,        0, "0"
HKR, ,                          TxFifo,            0, "8"
HKR, ,                          TxPerUnderrun,     0, "20000"
HKR, ,                          Ucode,             0, "0"
HKR, ,                          UnderrunRetry,     0, "1"
HKR, ,                          VLanMode,          0, "0"


[E100Server.CopyFiles]
