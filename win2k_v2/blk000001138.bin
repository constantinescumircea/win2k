AddReg          = CPQTRND5.reg, NC4621WOL.reg
BusType         = 5
Characteristics = 0x84
CopyFiles       = NC4621.Copyfiles

[NC4621.ndi.Services]
AddService      = CPQTRND5, 2, CPQTRND5.AddService, CPQTRND5.EventLog

[NC4621WOL.reg]
HKR, , WOL,  0, "0"

