;-----------------------------------------------------------------------------
; Common Parameters
;
[AddCommonParams]
HKR, ,                              BusType,            0, "5"
HKR, ,                              ReceiveBuffers,     0, "40"
HKR, ,                              UnderrunThreshold,  0, "0A"
HKR, ,                              UnderrunRetry,      0, "2"
HKR, ,                              InterruptMitigation,0, "0"
HKR, ,                              MapRegisters,       0, "20"
HKR, ,                              BurstLength,        0, "10"
