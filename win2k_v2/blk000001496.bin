tion Clients_Activate()
{
LoadScriptFile("idScriptUtil", "util.js");
g_docAll.idTrHeadMargin_Clients.style.display = 'block';
g_docAll.idTrHead_Clients.style.display = 'block';
g_docAll.idTrBody_Clients.style.display = 'block';
g_docAll.idTrBody2_Clients.style.display = 'block';
if (false == g_bClientsPageLoaded)
{
Clients_InitPage();
g_bClientsPageLoaded = true;
}
Clients_SetFocus();
}
function Clients_SetFocus()
{
var coll = g_docAll.tblClientsBody.all.tags("INPUT");
if (coll.length > 0)
{
coll(0).focus();
}
}
function Clients_Deactivate()
{
g_docAll.idTrHeadMargin_Clients.style.display = 'none';
g_docAll.idTrHead_Clients.style.display = 'none';
g_docAll.idTrBody_Clients.style.display = 'none';
g_docAll.idTrBody2_Clients.style.display = 'none';
}
function Clients_CloneElement(e)
{
e = e.cloneNode(true);
e.id = "";
return e;
}
function Clients_ConnectLabelToInput(eLabel, eInput)
{
eLabel.htmlFor = eInput.id = eInput.uniqueID;
}
function Clients_InitPage()
{
var eTypes = g_docAll.idClientList;
g_docAll.idCtlAppsDso.Exec("Add", "init clients", eTypes.sourceIndex);
if (eTypes._restricted)
{
tblClients.style.display = 'none';
idDivClientsHeader.style.display = 'none';
idDivClientsHeaderNonAdmin.className = "";
idClientsBodyHideIfNonAdmin.style.display = 'none';
idClientsBodyHideIfNonAdmin2.style.display = 'none';
}
else
{
var eBody = g_docAll.tblClientsBody;
for (var iType = 0; iType < eTypes.children.length; iType++)
{
var eType = eTypes.children(iType);
if (iType > 0)
{
var eRow = Clients_CloneElement(g_docAll.idClientSeparator);
eBody.appendChild(eRow);
}
var eHeader = Clients_CloneElement(g_docAll.idClientHeader);
eHeader.all("idClientCatName").innerText = eType._friendly;
eBody.appendChild(eHeader);
for (var iClient = 0; iClient < eType.children.length; iClient++)
{
var eClient = eType.children(iClient);
var eRow = Clients_CloneElement(g_docAll.idClientRow);
var eLabel = eRow.all("idShowIconLabel");
if (eClient._key)
{
var eShow = eRow.all("idShowIcon");
Clients_ConnectLabelToInput(eLabel, eShow);
eShow.defaultChecked = eClient._show;
}
else
{
eRow.all("idShowIconCell").innerText = "";
}
var sInput = "<INPUT type=radio _radio=1 onclick='Clients_OnRadioClick()' name=" + eClient._type;
if (!eClient._key) sInput += " checked";
sInput += ">";
var eInput = eClient.document.createElement(sInput);
eLabel = eRow.all("idClientRowInsert");
Clients_ConnectLabelToInput(eLabel, eInput);
eRow.all("idClientNameCell").appendChild(eInput);
eLabel.innerText = eClient._friendly;
eRow._master = eClient; 
eBody.appendChild(eRow);
}
}
}
}
function Clients_LocateLabelTarget(e, nm)
{
var eFound = null;
var eLabel = e.all(nm);
if (eLabel)
{
eFound = e.all(eLabel.htmlFor);
}
return eFound;
}
function Clients_OnRadioClick()
{
var eSrc = window.event.srcElement;
var ePrev = g_aPrevDisabledClass[eSrc.name];
if (ePrev)
{
ePrev.className = "";
}
var ePrev = g_aPrevDisabledInput[eSrc.name];
if (ePrev)
{
ePrev.disabled = false;
}
var eTr = window.event.srcElement;
while (eTr.className != "ClientRow") eTr = eTr.parentElement;
var eCheckbox = Clients_LocateLabelTarget(eTr, "idShowIconLabel");
if (eCheckbox)
{
eCheckbox.checked = true;
eCheckbox.disabled = true;
}
var eTd = eTr.all("idShowIconLabel");
if (eTd)
{
eTd.className = "disabled";
}
g_aPrevDisabledClass[eSrc.name] = eTd;
g_aPrevDisabledInput[eSrc.name] = eCheckbox;
}
function Clients_WalkApply(fApply)
{
var iActions = 0;
var i;
var coll = g_docAll.tblClientsBody.rows;
for (i = 0; i < coll.length; i++)
{
var eTr = coll(i);
var eCheckbox = Clients_LocateLabelTarget(eTr, "idShowIconLabel");
if (eCheckbox)
{
if (eCheckbox.checked != eCheckbox.defaultChecked)
{
iActions++;
if (fApply)
{
if (eCheckbox.checked)
{
g_docAll.idCtlAppsDso.Exec("Add", "show icons", eTr._master.sourceIndex);
}
else
{
g_docAll.idCtlAppsDso.Exec("Add", "hide icons", eTr._master.sourceIndex);
}
}
}
var eSetDefault = Clients_LocateLabelTarget(eTr, "idClientRowInsert");
if (eSetDefault.checked)
{
iActions++;
if (fApply)
{
g_docAll.idCtlAppsDso.Exec("Add", "set default client", eTr._master.sourceIndex);
}
}
}
}
return iActions;
}
function Clients_Apply()
{
var iActions = Clients_WalkApply(false);
if (iActions)
{
g_docAll.idCtlAppsDso.Exec("Add", "progressbar", iActions);
Clients_WalkApply(true);
g_docAll.idCtlAppsDso.Exec("Add", "progressbar", 0);
}
}
