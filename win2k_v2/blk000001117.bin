[Ec.CardEventLog.reg]
    HKR,,EventMessageFile,0x00020000,"%%SystemRoot%%\System32\Eicon\ecsrv.exe" ;now in \system32 bfr \eicon
    HKR,,TypesSupported, 0x00010001, 7


