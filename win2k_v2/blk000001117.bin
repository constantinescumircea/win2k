    HKR,,EventMessageFile,0x00020000,"%%SystemRoot%%\System32\IoLogMsg.dll;%%SystemRoot%%\System32\drivers\ecnb.sys" ;now in \system32\drivers
    HKR,,TypesSupported,0x00010001, 7

