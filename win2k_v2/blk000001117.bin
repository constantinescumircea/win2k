[Version]
Signature   = "$Windows NT$"
Class       = Net
ClassGUID   = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider    = %StrEicon%
LayoutFile  = layout.inf
DriverVer   = 09/21/1999,5.1.70.

[Manufacturer]
%StrEicon%=Cards

[ControlFlags]
ExcludeFromSelect=PCI\VEN_1133&DEV_B921
ExcludeFromSelect=PCI\VEN_1133&DEV_B922
ExcludeFromSelect=PCI\VEN_1133&DEV_B923
    
[Cards]     
    ; DisplayName     Section       DeviceID
    ; -----------     -------       -------- ;Passive card

%EcP92.DevDesc%     = EcP92,    PCI\VEN_1133&DEV_B921      
%EcP92.DevDesc%     = EcP92,    PCI\VEN_1133&DEV_B922
%EcP92.DevDesc%     = EcP92,    PCI\VEN_1133&DEV_B923
    
;------------------------------P92--------------------------------------------

[EcP92]
    AddReg=EcP92.reg
    CopyFiles=PDrvFiles,PPROGRAM-SUB_SCRIPTS,PVISUAL,PDOC,PPROGRAM
    Characteristics=0x84      ;NCF_PHYSICAL AND NCF_HAS_UI
    BusType=5

[EcP92.Services]
    AddService=ECCOMMDD,        0x2,    ECCOMMDD.AddService,        ECCOMMDD.EventLog
    AddService=Ecdtrace,        0,    Ecdtrace.AddService,        Ecdtrace.EventLog
    DelServices=ECCOMMDD
    DelServices=Ecdtrace

[EcP92.CoInstallers]
    CopyFiles=CoInstaller
    AddReg=EcP92.CoInstallers_AddReg

[EcP92.CoInstallers_AddReg]
    HKR,,CoInstallers32,0x00010000,"Ecpinst.dll,CoDeviceInstall"
 
[EcP92.reg]
    HKR,Ndi,Service,0,"ECCOMMDD"
    HKR,Ndi\Interfaces,UpperRange,0,"noupper"
    HKR,Ndi\Interfaces,LowerRange,0,"nolower"
    HKR,,EnumPropPages32,0,"EcPageX.dll,EnumPropPagesP"
    HKR,Parameters,BusType,0x00010001,5
    HKR,Parameters,DriverName,,"Eccommdd"
    HKR,Parameters,BusNumber,0x00010001,0
    HKR,Parameters,EiconCardType,0x00010001,1  ;Passed later when calling ECCARD2.exe 
    HKR,,EiconCardType,0x00010001,1 
 
;==================================================================
; Card Driver
;==================================================================

[Eccommdd.AddService]
    DisplayName     = %StrEccommddDisplay%
    ServiceType     = 1     ;SERVICE_KERNEL_DRIVER
    StartType       = 3     ;started on demand
    ErrorControl    = 1     ;SERVICE_ERROR_NORMAL
    ServiceBinary   = %12%\eccommdd.sys
    LoadOrderGroup  = NDIS

[Eccommdd.EventLog]
    AddReg = Eccommdd.AddEventLog.reg

[Eccommdd.AddEventLog.reg]
    HKR,,EventMessageFile,0x00020000,"%%SystemRoot%%\System32\IoLogMsg.dll;%%SystemRoot%%\System32\drivers\eccommdd.sys" 
    HKR,,TypesSupported,0x00010001, 7

;=====================================================================
; EcDTrace Driver
;=====================================================================

[Ecdtrace.AddService]
    DisplayName     = %StrEcdtraceDisplay%
    ServiceType     = 1     ;SERVICE_KERNEL_DRIVER
    StartType       = 3     ;started on demand
    ErrorControl    = 1     ;SERVICE_ERROR_NORMAL
    ServiceBinary   = %12%\ecdtrace.sys

[ecdtrace.EventLog]
    AddReg = ecdtrace.AddEventLog.reg

[ecdtrace.AddEventLog.reg]
    HKR,,EventMessageFile,0x00020000,"%%SystemRoot%%\System32\IoLogMsg.dll;%%SystemRoot%%\System32\drivers\ecdtrace.sys" 
    HKR,,TypesSupported,0x00010001, 7

