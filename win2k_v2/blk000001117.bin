;
; Installation inf for the Intel Corporation i81x graphics adapter.
;

[Version]
Signature="$CHICAGO$"
Provider=%MS%
LayoutFile=layout.inf
ClassGUID={4D36E968-E325-11CE-BFC1-08002BE10318}
Class=Display
;CatalogFile=i81xnt5.cat

[DestinationDirs]
DefaultDestDir   = 11
i81x.Miniport  = 12  ; drivers
i81x.Display   = 11  ; system32
;i81x.Config    = 11  ; config app
;i81x.OpenGL    = 11  ; OpenGL Drivers in System32
;
; Driver information
;

[Manufacturer]
%Intel%   = Intel.Mfg

[Intel.Mfg]
%Intel% 810 Graphics Controller Hub = i81x, PCI\VEN_8086&DEV_7123
%Intel% 810 Graphics Controller Hub = i81x, PCI\VEN_8086&DEV_7121
%Intel% 810 Graphics Controller Hub = i81x, PCI\VEN_8086&DEV_7125

;
; General installation section
;

[i81x]
;CopyFiles=i81x.Miniport, i81x.Display, i81x.Config, i81x.OpenGL
CopyFiles=i81x.Miniport, i81x.Display
;AddReg = i81x.ConfigSettings
DriverVer=09/30/1999

;
; File sections
;

[i81x.Miniport]
i81xnt5.sys

[i81x.Display]
i81xdnt5.dll

;[i81x.Config]
;i81xcnt5.dll
;EnuIgfx.lrc
;Enugfxcg.hlp
;msvcrt.dll,,,0x00000010
;i81xcfg.ico

;[i81x.OpenGL]
;i81xgdev.dll
;i81xgicd.dll

;
; Service Installation
;

[i81x.Services]
AddService = i81x, 0x00000002, i81x_Service_Inst, i81x_EventLog_Inst

[i81x_Service_Inst]
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 1                  ; SERVICE_SYSTEM_START
ErrorControl   = 0                  ; SERVICE_ERROR_IGNORE
LoadOrderGroup = Video
ServiceBinary  = %12%\i81xnt5.sys

[i81x_EventLog_Inst]
AddReg = i81x_EventLog_AddReg

[i81x_EventLog_AddReg]
HKR,,EventMessageFile,0x00020000,"%SystemRoot%\System32\IoLogMsg.dll;%SystemRoot%\System32\drivers\i81xnt5.sys"
HKR,,TypesSupported,0x00010001,7

[i81x.ConfigSettings]
;HKCR,"CLSID\{C24AC020-1A6A-11d2-9E12-00609445AED9}",,,"Control Panel Display Shell Extension"
;HKCR,"CLSID\{C24AC020-1A6A-11d2-9E12-00609445AED9}\InProcServer32",,,"%11%\i81xcnt5.dll"
;HKCR,"CLSID\{C24AC020-1A6A-11d2-9E12-00609445AED9}\InProcServer32","ThreadingModel",,"Apartment"
;HKLM,"Software\Microsoft\Windows\CurrentVersion\Controls Folder\Display\shellex\PropertySheetHandlers\i81xcnt5",,,"{C24AC020-1A6A-11d2-9E12-00609445AED9}"
;HKLM,"Software\Intel\igfxpw","Language",,"enu"

;
; Software Installation
;

[i81x.SoftwareSettings]
;AddReg = i81x_SoftwareDeviceSettings,i81x.OpenGLSoftwareSettings
AddReg = i81x_SoftwareDeviceSettings
DelReg = i81x_DelReg

[i81x_SoftwareDeviceSettings]
HKR,, InstalledDisplayDrivers,     %REG_MULTI_SZ%, i81xdnt5
HKR,, VgaCompatible,               %REG_DWORD%,    0
HKR,, CapabilityOverride,          %REG_DWORD%,    0

[i81x_DelReg]
HKLM,"SOFTWARE\Microsoft\Windows NT\CurrentVersion\OpenGLDrivers",i81x

[i81x.GeneralConfigData]
MaximumDeviceMemoryConfiguration = 64

;[i81x.OpenGLSoftwareSettings]
;HKLM,"Software\Microsoft\Windows NT\CurrentVersion\OpenGLDrivers\i81x","dll",%REG_SZ%,"i81xgicd"
;HKLM,"Software\Microsoft\Windows NT\CurrentVersion\OpenGLDrivers\i81x","Version",%REG_DWORD%,0x2
;HKLM,"Software\Microsoft\Windows NT\CurrentVersion\OpenGLDrivers\i81x","DriverVersion",%REG_DWORD%,0x1
;HKLM,"Software\Microsoft\Windows NT\CurrentVersion\OpenGLDrivers\i81x","Flags",%REG_DWORD%,0x3

[Strings]

;
; Non-Localizable Strings
;

REG_SZ         = 0x00000000
REG_MULTI_SZ   = 0x00010000
REG_EXPAND_SZ  = 0x00020000
REG_BINARY     = 0x00000001
REG_DWORD      = 0x00010001
SERVICEROOT    = System\CurrentControlSet\Services

;
; Localizable Strings
;

DiskId       = "Intel Corporation, Intel81x NT5 Installation Disk"
GraphAdap    = "Graphics Adapter"
