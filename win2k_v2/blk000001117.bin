    DisplayName     = %StrCardService%
    ServiceType     = 16    ;%SERVICE_WIN32_OWN_PROCESS% 
    StartType       = 2 
    ErrorControl    = 1     ;SERVICE_ERROR_NORMAL
    ServiceBinary   = %11%\Eicon\ecsrv.exe
;    Dependencies    = Ecnb
 
[Ec.CardEventLog]
