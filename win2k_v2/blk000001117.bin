[version]
Signature   = "$Windows NT$"
Class       = Net
ClassGUID   = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider    = %StrEicon%
LayoutFile  = layout.inf
DriverVer   = 09/21/1999,5.1.70.2

[Manufacturer]
    %StrEicon%=Options


[Options]

; DisplayName                 Section            DeviceID
; -----------                 -------            --------
%EcWanI%                    = EcWanI.Inst,      *EIC6970
%EcWan%                     = EcWan.Inst,       *EIC6972
%Router%                    = EcRout.Inst,      *EIC6969


;******************************************************************************************
; Router ------------

[EcRout.Inst]
    Characteristics = 0x1    ; NCF_VIRTUAL 
    MaxInstance     = 6
    AddReg          = EcRout.Reg,EcRout.Soft
    CopyFiles       = Files_Ecndis_DRV, Files_Ecndis_Root

[EcRout.Inst.CoInstallers]
;   CopyFiles=@Ecpinst.dll
    AddReg=EcRout.CoInst.reg

[EcRout.CoInst.reg]
    HKR,,CoInstallers32,0x00010000,"Ecpinst.dll,CoNdisDevInstall"

[EcRout.Inst.Services]        
    AddService = Ecndis,            0x2,    Ecndis.AddService,Ecndis.EventLog
    AddService = EiconNDISRouter,   0,      EiconNDISRouter.AddService,EiconNDISRouter.EventLog


[EcRout.Reg]
    HKR, Ndi,            Service,       0, "Ecndis"

    HKR, Ndi\Interfaces,    DefUpper,   0, "ndis5"
    HKR, Ndi\Interfaces,    DefLower,   0, "ethernet"
    HKR, Ndi\Interfaces,    UpperRange, 0, "ndis5"
    HKR, Ndi\Interfaces,    LowerRange, 0, "ethernet"

;used to be in parameters
    HKR,,EcPortNumber,0x00010001,1
    HKR,,LinkSpeed,0x00010001,128000


[Install-Rout]
    ;CopyFiles   = Files-Rtr, Files-Rtr-Drv
    ;AddReg      = Reg-Rtr-EventLogs, Reg-Rtr-Software

[Remove-Rout]
    ;DelReg      = Reg-Rtr-EventLogs, Reg-Rtr-Software


[EcRout.Soft]   ;temp software keys
    HKLM,Software\Eicon\Ecndis\CurrentVersion,Description,,"Eicon NDIS Interface Driver for Eicon WAN Adapter"
    HKLM,Software\Eicon\Ecndis\CurrentVersion,InUsePortList,,"1"
    HKLM,Software\Eicon\Ecndis\CurrentVersion,ListOfServices,,"Ecndis4"
    HKLM,Software\Eicon\Ecndis\CurrentVersion,MajorVersion,,"4"
    HKLM,Software\Eicon\Ecndis\CurrentVersion,MinorVersion,,"1"
    HKLM,Software\Eicon\Ecndis\CurrentVersion,RefCount,0x00010001,0x1
    HKLM,Software\Eicon\Ecndis\CurrentVersion,Revision,,"0"
    HKLM,Software\Eicon\Ecndis\CurrentVersion,ServiceName,,"Ecndis"
    HKLM,Software\Eicon\Ecndis\CurrentVersion,SoftwareType,,"service"
    HKLM,Software\Eicon\Ecndis\CurrentVersion,Title,,"Eicon Routing Services"


;-------------------------------------------------
;Router SERVICE ------------


[Ecndis.AddService]
    DisplayName     =   %StrEcndisDisplay%
    ServiceType     =   1 ;SERVICE_KERNEL_DRIVER
    StartType       =   2 ;SERVICE_AUTO_START
    ErrorControl    =   1 ;SERVICE_ERROR_NORMAL
    ServiceBinary   =   %12%\ecndis.sys
    LoadOrderGroup  =   NDIS
    AddReg          =   EiconCards.ServiceRegParams


[EiconCards.ServiceRegParams]
    HKR,Parameters,MaxPorts,                0x00010001, 2
    HKR,Parameters,MaxCircuits,             0x00010001, 16
    HKR,Parameters,MaxIPMappings,           0x00010001, 16
    HKR,Parameters,PortTxBuffers,           0x00010001, 6
    HKR,Parameters,XmitQueueSize,           0x00010001, 10
    HKR,Parameters,GlobalTxBuffers,         0x00010001, 0
    HKR,Parameters,LowTraceBuf,             0x00010001, 15
    HKR,Parameters,LowTraceData,            0x00010001, 128
    HKR,Parameters,UpTraceBuf,              0x00010001, 15
    HKR,Parameters,UpTraceData,             0x00010001, 128
    HKR,Parameters,EiconDbg,                0x00010001, 255
    HKR,Parameters,ApiEiconDbg,             0x00010001, 0
    HKR,Parameters,TcpSpoofMaxSession,      0x00010001, 300
    HKR,Parameters,TcpSpoofMaxIdle,         0x00010001, 7200
    HKR,Parameters,TcpSpoofMaxIdleSpoof,    0x00010001, 900
    HKR,Parameters,MaxIpFilters,            0x00010001, 16
    HKR,Parameters,LowerLevelTrace,         ,           "OFF"
    HKR,Parameters,UpperLevelTrace,         ,           "OFF"
    HKR,Parameters,EiconPath,               ,           "%%SystemRoot%%\System32"     ;eicon path


[Ecndis.EventLog]
    AddReg = Ecndis.AddEventLog.reg


[Ecndis.AddEventLog.reg]
    HKR,,EventMessageFile,0x00020000,"%%SystemRoot%%\System32\netevent.dll;%%SystemRoot%%\System32\drivers\Ecndis.sys"
    HKR,,TypesSupported,0x00010001,7    
    

;----------------------------

[EiconNDISRouter.AddService]
    DisplayName     =   %StrEiconNDISRouterDisplay%
    ServiceType     =   16 
    StartType       =   2   ;SERVICE_AUTO_START
    ErrorControl    =   1   ;SERVICE_ERROR_NORMAL
    ServiceBinary   =   %11%\Eicon\ecrtr.exe        ;C:\EICON\WSNT\ecrtr.exe
    LoadOrderGroup  =   ""
    Dependencies    =   Ecnb,Ecndis,EiconCards
    AddReg          =   EiconNDISRouter.ServiceRegParams

[EiconNDISRouter.ServiceRegParams]

[EiconNDISRouter.EventLog]
    AddReg = EiconNDISRouter.AddEventLog.reg

[EiconNDISRouter.AddEventLog.reg]
    HKR,,EventMessageFile,0x00020000,"%%SystemRoot%%\System32\Eicon\ecrtr.exe" ;now in \system32 bfr \eicon
    HKR,,TypesSupported,0x00010001,7    



;******************************************************************************************
; ISDN WAN Miniport ------------


[EcWanI.Inst]
    Characteristics = 0x1    ; NCF_VIRTUAL 
    AddReg          = EcWanI.Reg
    CopyFiles       = Files_EcWanI_DRV


[EcWanI.Inst.Hw]
    AddReg = Reg-LowerFilter-NdisTapi
             


[EcWanI.Reg]
    HKR, Ndi,            Service,           0,          "Ecwani"

    HKR, Ndi\Interfaces, UpperRange,        0,          "ndiswan"
    HKR, Ndi\Interfaces, LowerRange,        0,          "nolower"


    HKR,,               WanEndpoints,0x00010001,            2   

