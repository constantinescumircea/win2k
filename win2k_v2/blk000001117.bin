    HKR, ,  BusNumber,  0x00010001,         0
    HKR, ,  BusType,    0x00010001,         0
    HKR, ,  DeviceName,           ,         "ECWANI"    
    HKR, ,  LineType,   0x00010001,         1   
    HKR, ,  MediaType,            ,         "ISDN"

;-------

    HKR, ,AddressList,          0x00010000  ,4-1-0,4-2-0
    HKR, ,Async,                0x00010000  ,0
    HKR, ,InitiateCallClear,    0x00010000  ,0
    HKR, ,SendX3Profile,        0x00010000  ,0
    HKR, ,TOA,                  0x00010000  ,0
    HKR, ,X25Profile,           0x00010000  ,<none>,<none>
    HKR, ,X3Profile,            0x00010000  ,<none>,<none>


