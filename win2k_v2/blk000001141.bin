; NETOCE55.INF
;
; Version 1.15 (NDIS 5.0) for Windows 2000
;
; Olicom Network Interface Card
; Olicom RapidFire Ethernet 10/100 PCI Adapters, NDIS Miniport 5.0
;
; Copyright 1999, Olicom A/S

[version]
Signature  = "$Windows NT$"
Class      = Net
ClassGUID  = {4D36E972-E325-11CE-BFC1-08002BE10318}
Provider   = %MSFT%
LayoutFile = layout.inf
DriverVer=10/19/1999,1.15.0.1

[Manufacturer]
%V_OLICOM%=Olicom

[ControlFlags]
ExcludeFromSelect   = PCI\VEN_1011&DEV_0019&SUBSYS_0016108D
ExcludeFromSelect   = PCI\VEN_1011&DEV_0019&SUBSYS_0017108D

[Olicom]
%OC2327.DeviceDesc%= OC2327.ndi,	   PCI\VEN_1011&DEV_0019&SUBSYS_0016108D  ;Olicom 2327
%OC2250.DeviceDesc%= OC2250.ndi,	   PCI\VEN_1011&DEV_0019&SUBSYS_0017108D  ;Olicom 2250

;-------------------------------------------------------------------------------
; Olicom RapidFire 2327 Ethernet 10/100 PCI Adapter
;
[OC2327.ndi.NT]
AddReg          = occommonNT.reg, oc2327NT.reg
CopyFiles       = oce5xNT.CopyFiles
Characteristics = 0x84
BusType 	= 5

[OC2327.ndi.NT.Services]
AddService      = oce5xnd5, 2, oce5xnd5NT.AddService, oce5xnd5NT.EventLog


;-------------------------------------------------------------------------------
; Olicom GoCard 2250 Ethernet 10/100 CardBus PC Card"
;
[OC2250.ndi.NT]
AddReg          = occommonNT.reg, oc2250NT.reg
CopyFiles       = oce5xNT.CopyFiles
Characteristics = 0x84
BusType 	= 5

[OC2250.ndi.NT.Services]
AddService      = oce5xnd5, 2, oce5xnd5NT.AddService, oce5xnd5NT.EventLog

;-------------------------------------------------------------------------------
; OC Driver common registry
;
[occommonNT.reg]
; Interfaces and Parameters
HKR, Ndi,                       Service,    0, "oce5xnd5"
HKR, Ndi\Interfaces,            UpperRange, 0, "ndis5"
HKR, Ndi\Interfaces,            LowerRange, 0, "ethernet"

;Misc.
HKR,,MediaType,0,"1"
HKR,,OSType,0,"800"

;----------------------------------------------------------------------------
; OC 2327 Driver registry
;
[oc2327NT.reg]
HKR,,AdapterType,0,"5"
HKR,,AdapterCFID,0,"00191011"
HKR, Ndi, HelpText, 0, %Help2327ND5%

;params

;Receive Buffers
HKR, Ndi\params\ReceiveBuffers,     ParamDesc,  0, %ReceiveBuffers%
HKR, Ndi\params\ReceiveBuffers,     type,       0, "enum"
HKR, Ndi\params\ReceiveBuffers,     default,    0, "16"
HKR, Ndi\params\ReceiveBuffers\enum,16,         0, "16"
HKR, Ndi\params\ReceiveBuffers\enum,24,         0, "24"
HKR, Ndi\params\ReceiveBuffers\enum,32,         0, "32"
HKR, Ndi\params\ReceiveBuffers\enum,40,         0, "40"
HKR, Ndi\params\ReceiveBuffers\enum,48,         0, "48"
HKR, Ndi\params\ReceiveBuffers\enum,56,         0, "56"
HKR, Ndi\params\ReceiveBuffers\enum,64,         0, "64"
;Use Extended PCI Commands
HKR, Ndi\params\ExtendedPCI,        ParamDesc,  0, %ExtendedPCI%
HKR, Ndi\params\ExtendedPCI,        default,    0, "1"
HKR, Ndi\params\ExtendedPCI,        type,       0, "enum"
HKR, Ndi\params\ExtendedPCI\enum,   0,          0, %No%
HKR, Ndi\params\ExtendedPCI\enum,   1,          0, %Yes%
;Connection Type
HKR, Ndi\params\ConnectionType,     ParamDesc,  0, %ConnectionType%
HKR, Ndi\params\ConnectionType,     default,    0, "0"
HKR, Ndi\params\ConnectionType,     type,       0, "enum"
HKR, Ndi\params\ConnectionType\enum,0,          0, %auto%
HKR, Ndi\params\ConnectionType\enum,8,          0, %CT100Half%
HKR, Ndi\params\ConnectionType\enum,9,          0, %CT100Full%
HKR, Ndi\params\ConnectionType\enum,2,          0, %CT10Half%
HKR, Ndi\params\ConnectionType\enum,3,          0, %CT10Full%
; NetworkAddress
HKR, NDI\params\NetworkAddress,     ParamDesc,  0, %NetworkAddress%
HKR, NDI\params\NetworkAddress,     type,       0, "edit"
HKR, NDI\params\NetworkAddress,     LimitText,  0, "18"
HKR, NDI\params\NetworkAddress,     UpperCase,  0, "1"
HKR, Ndi\params\NetworkAddress,     default,    0, ""
HKR, Ndi\params\NetworkAddress,     optional,   0, "1"


;----------------------------------------------------------------------------
; OC 2250 Driver registry
;
[oc2250NT.reg]
HKR,,AdapterType,0,"5"
HKR,,AdapterCFID,0,"00191011"
HKR, Ndi, HelpText, 0, %Help2250ND5%

;params

;Receive Buffers
HKR, Ndi\params\ReceiveBuffers,     ParamDesc,  0, %ReceiveBuffers%
HKR, Ndi\params\ReceiveBuffers,     type,       0, "enum"
