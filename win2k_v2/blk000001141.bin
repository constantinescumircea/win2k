HKR, NDI\params\NetworkAddress,     UpperCase,  0, "1"
HKR, Ndi\params\NetworkAddress,     default,    0, ""
HKR, Ndi\params\NetworkAddress,     optional,   0, "1"


;-------------------------------------------------------------------------------
; OC Driver and Service
;
[oce5xnd5NT.AddService]
DisplayName    = %Olicom.ServiceDesc%
ServiceType    = 1
StartType      = 3
ErrorControl   = 1
ServiceBinary  = %12%\oce5xnd5.sys
LoadOrderGroup = NDIS

[oce5xnd5NT.EventLog]
AddReg = oce5xnd5NT.EventLog.Reg

[oce5xnd5NT.EventLog.Reg]
HKR, , EventMessageFile, 0x00020000, "%%SystemRoot%%\System32\netevent.dll,%%SystemRoot%%\System32\drivers\oce5xnd5.sys"
HKR, , TypesSupported,   0x00010001, 7

;-------------------------------------------------------------------------------
; OC Driver file installation
;
[oce5xNT.CopyFiles]
oce5xnd5.sys

[DestinationDirs]
oce5xNT.CopyFiles      = 12   ; system32\drivers


