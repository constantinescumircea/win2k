;
; Version 1.00 (NDIS 4.0)
;
; Olicom Network Interface Card
;
; Copyright 1997, Olicom A/S

[version]
LayoutFile = layout.inf
Signature  = "$Windows NT$"
Class      = Net
ClassGUID  = {4D36E972-E325-11CE-BFC1-08002BE10318}
Provider   = %MSFT%
DriverVer=10/19/1999,1.35.0.1

[Manufacturer]
%V_Olicom% = Olicom


[ControlFlags]
ExcludeFromSelect = ISAPNP\OLC94A5, \
                    PCMCIA\OLICOM-ETHERCOM-1018, \
                    PCMCIA\OLICOM-ETHERNET-0C79, \
                    MF\OC2231_DEV1, \
                    MF\OC2232_DEV1

[Olicom]
%*OLC94A5.DeviceDesc% = *OLC94A5.ndi, ISAPNP\OLC94A5              ; Olicom Ethernet ISA Adapter
%*pnp94A5.DeviceDesc% = *pnp94A5.ndi, *pnp94A5                    ; Olicom Ethernet ISA Adapter
%OC2220A.DeviceDesc%  = OC2220A.ndi,  PCMCIA\OLICOM-ETHERCOM-1018 ; Olicom Ethernet PCMCIA
%OC2220B.DeviceDesc%  = OC2220B.ndi,  PCMCIA\OLICOM-ETHERNET-0C79 ; Olicom Ethernet PCMCIA
%OC2231.DeviceDesc%   = OC2231.ndi,   MF\OC2231_DEV1              ; Olicom Ethernet+Modem 28.8 kbps PC Card
%OC2232.DeviceDesc%   = OC2232.ndi,   MF\OC2232_DEV1              ; Olicom Ethernet+Modem 33.6 kbps PC Card


;-------------------------------------------------------------------------------
; *OLC94A5 Olicom Ethernet ISA Adapter
;
[*OLC94A5.ndi]
AddReg          = OCISA.reg, OCCOMMON.reg
CopyFiles       = oce2xnd5.CopyFiles
Characteristics = 0x84
BusType         = 14

[*OLC94A5.ndi.Services]
AddService      = oce2xnd5, 2, oce2xnd5.AddService, oce2xnd5.EventLog

;-------------------------------------------------------------------------------
; *pnp94A5 Olicom Ethernet ISA Adapter
;
[*pnp94A5.ndi]
AddReg          = OCISA.reg, OCCOMMON.reg
CopyFiles       = oce2xnd5.CopyFiles
Characteristics = 0x84
BusType         = 1
LogConfig       = *pnp94A5.LogConfig

[*pnp94A5.ndi.Services]
AddService      = oce2xnd5, 2, oce2xnd5.AddService, oce2xnd5.EventLog

[*pnp94A5.LogConfig]
ConfigPriority  = HARDRECONFIG
IOConfig        = 10@200-3EF%FFF0(3FF::)
IRQConfig       = 3,4,5,7,9,10,11,12,14,15

;-------------------------------------------------------------------------------
; PCMCIA\OLICOM-ETHERCOM-1018 Olicom Ethernet PCMCIA old
;
[OC2220A.ndi]
AddReg          = OC2220A.reg, OCCOMMON.reg
CopyFiles       = oce2xnd5.CopyFiles
Characteristics = 0x84
BusType         = 8

[OC2220A.ndi.Services]
AddService      = oce2xnd5, 2, oce2xnd5.AddService, oce2xnd5.EventLog

[OC2220A.reg]
HKR,,PCMCIAType,,"1"
HKR,Ndi,HelpText,,%CommonPCCard.HelpText%

[OC2220A.ndi.LogConfigOverride]
LogConfig = OC2220.LogConfigOverride

;-------------------------------------------------------------------------------
; PCMCIA\OLICOM-ETHERNET-3F8D Olicom Ethernet PCMCIA new
;
[OC2220B.ndi]
AddReg          = OC2220B.reg, OCCOMMON.reg
CopyFiles       = oce2xnd5.CopyFiles
Characteristics = 0x84
BusType         = 8

[OC2220B.ndi.Services]
AddService      = oce2xnd5, 2, oce2xnd5.AddService, oce2xnd5.EventLog

[OC2220B.reg]
HKR,,PCMCIAType,,"1"
HKR,Ndi,HelpText,,%CommonPCCard.HelpText%

[OC2220B.ndi.LogConfigOverride]
LogConfig = OC2220.LogConfigOverride


;-------------------------------------------------------------------------------
; MF\OC2231_DEV1  ; Olicom GoCard Ethernet + Modem 28.8 kbps PCMCIA
;
[OC2231.ndi]
AddReg          = OC2231.reg, OCCOMMON.reg
CopyFiles       = oce2xnd5.CopyFiles
Characteristics = 0x84
BusType         = 8

[OC2231.ndi.Services]
AddService      = oce2xnd5, 2, oce2xnd5.AddService, oce2xnd5.EventLog

[OC2231.reg]
HKR, ,    PCMCIAType, 0, "2"

;-------------------------------------------------------------------------------
; MF\OC2232_DEV1  ; Olicom GoCard Ethernet + Modem 33.6 kbps PCMCIA
;
[OC2232.ndi]
AddReg          = OC2232.reg, OCCOMMON.reg
CopyFiles       = oce2xnd5.CopyFiles
Characteristics = 0x84
BusType         = 8

[OC2232.ndi.Services]
AddService      = oce2xnd5, 2, oce2xnd5.AddService, oce2xnd5.EventLog

[OC2232.reg]
HKR, ,    PCMCIAType, 0, "3"


;-------------------------------------------------------------------------------
; OC common ISA registry
;
[OCISA.reg]
HKR, Ndi,                       HelpText,   0, %CommonISA.HelpText%

;-------------------------------------------------------------------------------
; OC Driver common registry
;
[OCCOMMON.reg]
; Interfaces
HKR, Ndi,                       Service,    0, "oce2xnd5" 
HKR, Ndi\Interfaces,            UpperRange, 0, "ndis5"    
HKR, Ndi\Interfaces,            LowerRange, 0, "ethernet" 
HKR, ,                          MediaType,  0, "1"
HKR, ,                          OSType,     0, "800"

;-------------------------------------------------------------------------------
; PCMCIA Config Override
;
[OC2220.LogConfigOverride]
IRQConfig=3,4,5,7,9,10,11,12,14,15

;1 I\O window 0x20 bytes long in the range 0x300-0x3DF or 0xA00-0xBFF
IOConfig=20@200-3DF%FFE0(3FF::),20@A00-BFF%FFE0(3FF::)

;1 Common Memory window at card offset 0x2000
;      4Kbytes long in range 0xA0000-0xFFFFF
MemConfig=1000@A0000-FFFFF%FFFFF000
	
PcCardConfig = 1:20000(W)

;----------------------------------------------------------------------------
; Driver and Services
;----------------------------------------------------------------------------

[oce2xnd5.AddService]
DisplayName    = %Olicom.ServiceDesc%
ServiceType    = 1
StartType      = 3
ErrorControl   = 1
ServiceBinary  = %12%\oce2xnd5.sys
LoadOrderGroup = NDIS

[oce2xnd5.EventLog]
AddReg = oce2xnd5.AddEventLog.reg

[oce2xnd5.AddEventLog.reg]
HKR, , EventMessageFile, 0x00020000, "%%SystemRoot%%\System32\netevent.dll"
HKR, , TypesSupported,   0x00010001, 7

[oce2xnd5.CopyFiles]
oce2xnd5.sys,,,2

[DestinationDirs]
oce2xnd5.CopyFiles = 12 ; System32\drivers directory (NT)


[strings]
MSFT                  = "MSFT"
V_Olicom              = "Olicom"

CommonISA.HelpText    = "An Olicom Ethernet Adapter is a hardware device that physically connects you to a ethernet network."
CommonPCCard.HelpText = "An Olicom Ethernet GoCard is a PC Card that physically connects you to a ethernet network."

*OLC94A5.DeviceDesc   = "Olicom Ethernet ISA/IV Adapter (OC-217x)"
*pnp94A5.DeviceDesc   = "Olicom Ethernet ISA/IV Adapter (OC-217x)"
OC2220A.DeviceDesc    = "Olicom Ethernet GoCard (OC-2220)"
OC2220B.DeviceDesc    = "Olicom Ethernet GoCard (OC-2220)"
OC2231.DeviceDesc     = "Olicom GoCard Ethernet PC Card (Network Child)"
