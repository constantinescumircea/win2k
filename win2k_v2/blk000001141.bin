;
; Version : 1.02 (NDIS 4.0)
;
; Olicom Ethernet ISA 10/100 Adapters, NDIS Miniport 4.0
;
; Copyright 1997, Olicom A/S

[version]
LayoutFile = layout.inf
Signature  = "$Windows NT$"
Class      = Net
ClassGUID  = {4D36E972-E325-11CE-BFC1-08002BE10318}
Provider   = %msft%
DriverVer=10/19/1999,1.6.0.1

[Manufacturer]
%V_Olicom% = Olicom


[ControlFlags]
ExcludeFromSelect = ISAPNP\OLC9510, ISAPNP\OLC9511

[OLICOM]
%*pnp9510.DeviceDesc% = *pnp9510.ndi, ISAPNP\OLC9510 ; Olicom Ethernet ISA 10/100 Adapter (OC-2375)
%*ISA9510.DeviceDesc% = *ISA9510.ndi, *pnp9510       ; Olicom Ethernet ISA 10/100 Adapter (OC-2375) in Legacy Mode
%*OLC9511.DeviceDesc% = *OLC9511.ndi, ISAPNP\OLC9511 ; Olicom Ethernet ISA 10/100 Adapter (OC-2376) 
%*pnp9511.DeviceDesc% = *pnp9511.ndi, *pnp9511       ; Olicom Ethernet ISA 10/100 Adapter (OC-2376) in Legacy Mode

;-------------------------------------------------------------------------------
; *pnp9510 Olicom Ethernet ISA Adapter (OC-2375)
;
[*pnp9510.ndi]
AddReg          = OCCOMMON.reg
CopyFiles       = oce3x.CopyFiles
Characteristics = 0x84
BusType 	= 14

[*pnp9510.ndi.Services]
AddService      = oce3xnd5, 2, oce3xnd5.AddService, oce3xnd5.EventLog

;-------------------------------------------------------------------------------
; *pnp9510 Olicom Ethernet ISA Adapter (OC-2375) in Legacy Mode
;
[*ISA9510.ndi]
AddReg          = OCCOMMON.reg
CopyFiles       = oce3x.CopyFiles
Characteristics = 0x84
BusType 	= 1
LogConfig       = *ISA9510.LogConfig

[*ISA9510.ndi.Services]
AddService      = oce3xnd5, 2, oce3xnd5.AddService, oce3xnd5.EventLog

[*ISA9510.LogConfig]
ConfigPriority  = HARDRECONFIG
IOConfig        = 20@0200-03FF%FFE0(3FF::)
IRQConfig       = 3,5,7,9,10,11,12,15

;-------------------------------------------------------------------------------
; *OLC9511 Olicom Ethernet ISA Adapter (OC-2376)
;
[*OLC9511.ndi]
AddReg          = OCCOMMON.reg
CopyFiles       = oce3x.CopyFiles
Characteristics = 0x84
BusType 	= 14

[*OLC9511.ndi.Services]
AddService      = oce3xnd5, 2, oce3xnd5.AddService, oce3xnd5.EventLog

;-------------------------------------------------------------------------------
; *OLC9511 Olicom Ethernet ISA Adapter (OC-2376) in Legacy Mode
;
[*pnp9511.ndi]
AddReg          = OCCOMMON.reg
CopyFiles       = oce3x.CopyFiles
Characteristics = 0x84
BusType 	= 1
LogConfig       = *pnp9511.LogConfig

[*pnp9511.ndi.Services]
AddService      = oce3xnd5, 2, oce3xnd5.AddService, oce3xnd5.EventLog

[*pnp9511.LogConfig]
ConfigPriority  = HARDRECONFIG
IOConfig        = 20@0200-03FF%FFE0(3FF::)
IRQConfig       = 3,5,7,9,10,11,12,15

;-------------------------------------------------------------------------------
; OC Common registry parameter
;
[OCCOMMON.reg]
; Interfaces
HKR, Ndi\Interfaces,            UpperRange, 0, "ndis5"
HKR, Ndi\Interfaces,            LowerRange, 0, "ethernet"

; Parameters
HKR, Ndi,                       Service,    0, "oce3xnd5"
HKR, Ndi,                       HelpText,   0, %Common.HelpText%
HKR, ,                          MediaType,  0, "1"
HKR, ,                          OSType,     0, "800"

;Speed
HKR, Ndi\params\Speed,          ParamDesc,  0, %Speed%
HKR, Ndi\params\Speed,          default,    0, "0"
HKR, Ndi\params\Speed,          type,       0, "enum"
HKR, Ndi\params\Speed\enum,     0,          0, %Auto%
HKR, Ndi\params\Speed\enum,     10,         0, "10 Mbps" 
HKR, Ndi\params\Speed\enum,     100,        0, "100 Mbps" 

;Duplex
HKR, Ndi\params\Duplex,         ParamDesc,  0, %Duplex%
HKR, Ndi\params\Duplex,         default,    0, "0"
HKR, Ndi\params\Duplex,         type,       0, "enum"
HKR, Ndi\params\Duplex\enum,    0,          0, %Auto%
HKR, Ndi\params\Duplex\enum,    half,       0, %HD%
HKR, Ndi\params\Duplex\enum,    full,       0, %FD%

;-------------------------------------------------------------------------------
; OC services and driver
;
[oce3xnd5.AddService]
DisplayName    = %Olicom.ServiceDesc%
ServiceType    = 1
StartType      = 3
ErrorControl   = 1
ServiceBinary  = %12%\oce3xnd5.sys
LoadOrderGroup = NDIS

[oce3xnd5.EventLog]
AddReg=oce3xnd5.AddEventLog.reg

[oce3xnd5.AddEventLog.reg]
HKR,,EventMessageFile, 0x00020000, "%%SystemRoot%%\System32\netevent.dll"
HKR,,TypesSupported,   0x00010001, 7

[oce3x.CopyFiles]
oce3xnd5.sys

[DestinationDirs]
oce3x.CopyFiles   = 12


;-------------------------------------------------------------------------------
; Localizable Strings
;
[strings]
msft                = "Microsoft"
V_Olicom            = "Olicom"

Speed               = "LAN Speed"
Duplex              = "Duplex"
Auto                = "Automatic"
HD                  = "Half Duplex"
FD                  = "Full Duplex"

Common.HelpText     = "An Olicom Ethernet Adapter is a hardware device that physically connects you to a ethernet network."

*pnp9510.DeviceDesc = "Olicom Ethernet ISA 10/100 Adapter (OC-2375)"
*ISA9510.DeviceDesc = "Olicom Ethernet ISA 10/100 Adapter (OC-2375) in Legacy Mode"
*OLC9511.DeviceDesc = "Olicom Ethernet ISA 10/100 Adapter (OC-2376)"
